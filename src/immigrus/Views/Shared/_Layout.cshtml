<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IMMIGRUS</title>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <link href="/img/favicon.144x144.html" rel="apple-touch-icon" type="image/png" sizes="144x144">
    <link href="/img/favicon.114x114.html" rel="apple-touch-icon" type="image/png" sizes="114x114">
    <link href="/img/favicon.72x72.html" rel="apple-touch-icon" type="image/png" sizes="72x72">
    <link href="/img/favicon.57x57.html" rel="apple-touch-icon" type="image/png">
    <link href="/img/favicon.html" rel="icon" type="image/png">
    <link href="/img/favicon-2.html" rel="shortcut icon">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    
    <link rel="stylesheet" href="/css/lib/bootstrap-table/bootstrap-table.min.css">
    <link rel="stylesheet" href="/css/lib/font-awesome/font-awesome.min.css">
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.common-material.min.css" />
    <link rel="stylesheet" href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.material.min.css" />

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link href="/css/style.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />

    @RenderSection("scriptsh", required: false)

    @*@Html.ApplicationInsightsJavaScript(TelemetryConfiguration)*@
</head>
<body class="with-side-menu">
    <header class="site-header">
        <div class="container-fluid">
            <a asp-controller="Home" asp-action="Index" class="site-logo">
                <img class="hidden-md-down" src="/img/logo-2.png" alt="">
                <img class="hidden-lg-up" src="/img/logo-2-mob.png" alt="">
            </a>
            @await Html.PartialAsync("_LoginPartial")
        </div><!--.container-fluid-->
    </header><!--.site-header-->
    <div class="mobile-menu-left-overlay"></div>
    <nav class="side-menu">
        <ul class="side-menu-list">
            <li class="grey with-sub">
                <a asp-controller="Home" asp-action="Index">
                    <i class="font-icon font-icon-dashboard"></i>
                    <span class="lbl">Accueil</span>
                </a>
            </li>
            <li class="brown with-sub">
                <span>
                    <i class="font-icon glyphicon glyphicon-tint"></i>
                    <span class="lbl">Faq</span>
                </span>
                <ul>
                    <li><a asp-controller="Faqs" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                    <li><a asp-controller="Faqs" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                </ul>
            </li>
            <li class="brown with-sub">
                <span>
                    <i class="font-icon glyphicon glyphicon-tint"></i>
                    <span class="lbl">Actualité</span>
                </span>
                <ul>
                    <li><a asp-controller="Actualites" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                    <li><a asp-controller="Actualites" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                </ul>
            </li>
            <li class="brown with-sub">
                <span>
                    <i class="font-icon glyphicon glyphicon-tint"></i>
                    <span class="lbl">Témoignages</span>
                </span>
                <ul>
                    <li><a asp-controller="Temoignages" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                    <li><a asp-controller="Temoignages" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                </ul>
            </li>
            <li class="brown with-sub">
                <span>
                    <i class="font-icon glyphicon glyphicon-tint"></i>
                    <span class="lbl">Photos</span>
                </span>
                <ul>
                    <li><a asp-controller="Photos" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                    <li><a asp-controller="Photos" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                </ul>
            </li>
            <li class="brown with-sub ">
                <span>
                    <i class="font-icon glyphicon glyphicon-tint"></i>
                    <span class="lbl">Flash Info</span>
                </span>
                <ul>
                    <li><a asp-controller="FlashInfo" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                    <li><a asp-controller="FlashInfo" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                </ul>
            </li>

            <li class="brown with-sub">
                <span>
                    <i class="font-icon font-icon-user"></i>
                    <span class="lbl">Gestion des Clients</span>
                </span>
                <ul>
                    <li><a asp-controller="Account" asp-action="CreerInscription"><span class="lbl">Inscription</span></a></li>

                    <li><a asp-controller="Account" asp-action="ListeInscription"><span class="lbl">Liste Inscription</span></a></li>
                    <li><a asp-controller="Account" asp-action="ListeDV"><span class="lbl">Liste DV</span></a></li>
                    <li><a asp-controller="Account" asp-action="ListeConfirmationNumber"><span class="lbl">Liste Postulant avc CN</span></a></li>
                   @* <li><a asp-controller="Account" asp-action="ListeResultat"><span class="lbl">Liste Resultat</span></a></li>*@
                    <li><a asp-controller="Account" asp-action="ListeClientBloque"><span class="lbl">Liste Clients Bloqu&eacute;s</span></a></li>
                </ul>
            </li>

            <li class="brown with-sub">
                <span>
                    <i class="font-icon font-icon-user"></i>
                    <span class="lbl">Resultats</span>
                </span>
                <ul>
                    <li><a asp-controller="Account" asp-action="ListeResultatEchec"><span class="lbl">Liste Echec</span></a></li>

                    <li><a asp-controller="Account" asp-action="ListeResultatAdmis"><span class="lbl">Liste Admis</span></a></li>
                                   </ul>
            </li>
            <li class="brown with-sub">
                <span>
                    <i class="font-icon font-icon-user"></i>
                    <span class="lbl">Infos Clients</span>
                </span>
                <ul>
                    <li><a asp-controller="Account" asp-action="ListeClients"><span class="lbl">Liste des clients</span></a></li>

                    <li><a asp-controller="Account" asp-action="ListeClientsArchives"><span class="lbl">Liste Clients Archiv&eacute;s</span></a></li>
                </ul>
            </li>

        </ul>

        <section>
            <header class="side-menu-title">Tags</header>
            <ul class="side-menu-list">
                <li class="brown with-sub">
                    <span>
                        <i class="font-icon glyphicon glyphicon-tint"></i>
                        <span class="lbl">Flash Info</span>
                    </span>
                    <ul>
                        <li><a asp-controller="FlashInfos" asp-action="Create"><span class="lbl">Nouvel Enregistrement</span></a></li>
                        <li><a asp-controller="FlashInfos" asp-action="Index"><span class="lbl">Liste des Enregistrements</span></a></li>
                    </ul>
                </li>
            </ul>
        </section>
    </nav><!--.side-menu-->
    <div class="page-content">
        <div class="container-fluid">
            @RenderBody()
        </div><!--.container-fluid-->
    </div><!--.page-content-->

    <script src="/js/lib/jquery/jquery.min.js"></script>
    <script src="/js/lib/tether/tether.min.js"></script>
    <script src="/js/lib/bootstrap/bootstrap.min.js"></script>
    <script src="/js/plugins.js"></script>
    

    @RenderSection("scripts", required: false)


    <script src="/js/app.js"></script>

    

   
</body>
</html>
